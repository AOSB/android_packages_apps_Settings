<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        android:key="power_saver_toggles_performance_key"
        android:title="@string/power_saver_toggles_performance_title">

        <CheckBoxPreference
            android:key="power_saver_toggles_cpu"
            android:title="@string/power_saver_toggles_cpu_title"
